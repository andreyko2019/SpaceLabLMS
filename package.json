{
  "name": "spacelablms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "student:dev": "pnpm --filter \"{apps/student/**}\" dev",
    "admin:dev": "pnpm --filter \"{apps/student/**}\" dev",
    "student:build": "cd apps/student && npm run build",
    "admin:build": "cd apps/admin && npm run build",
    "student:preview": "cd apps/student && npm run preview",
    "admin:preview": "cd apps/admin && npm run preview",
    "prebuild": "pnpm -r lint",
    "build": "pnpm -r --stream build",
    "admin:deploy": "gh-pages -d apps/admin/dist -b admin-site",
    "student:deploy": "gh-pages -d apps/student/dist -b student-site"
  },
  "dependencies": {
    "@vee-validate/yup": "^4.12.8",
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.8",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2",
    "vue-skeletor": "^1.0.6",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^9.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "gh-pages": "^6.1.1",
    "pnpm": "^9.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.77.1",
    "sass-loader": "^14.2.1",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
